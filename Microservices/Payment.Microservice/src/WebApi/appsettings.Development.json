{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=defaultdb;Username=postgres;Password=password"
  },
  "DatabaseOptions": {
    "MaxRetryCount": 3,
    "CommandTimeout": 30,
    "EnableDetailedErrors": false,
    "EnableSensitiveDataLogging": true
  },
  "VNPay": {
    "TmnCode": "",
    "HashSecret": "",
    "BaseUrl": "",
    "ReturnWalletUrl": "",
    "ReturnBookingUrl": "",
    "IpnUrl": ""
  },
  "Momo": {
    "PartnerCode": "",
    "PartnerName": "",
    "StoreId": "",
    "AccessKey": "",
    "SecretKey": "",
    "CreatePaymentUrl": "",
    "RedirectWalletUrl": "",
    "RedirectBookingUrl": "",
    "IpnUrl": "",
    "RequestType": "captureWallet",
    "Lang": "vi",
    "AutoCapture": true,
    "ExtraDataTemplate": "{\"paymentId\":\"{paymentId}\",\"userId\":\"{userId}\",\"flow\":\"{flow}\"}"
  },
  "PayOs": {
    "ClientId": "",
    "ApiKey": "",
    "ChecksumKey": "",
    "ReturnWalletUrl": "https://localhost/payment/payos/wallet-return?paymentId={paymentId}&orderCode={orderCode}",
    "CancelWalletUrl": "https://localhost/payment/payos/wallet-cancel?paymentId={paymentId}&orderCode={orderCode}",
    "ReturnBookingUrl": "https://localhost/payment/payos/booking-return?paymentId={paymentId}&orderCode={orderCode}&bookingId={bookingId}",
    "CancelBookingUrl": "https://localhost/payment/payos/booking-cancel?paymentId={paymentId}&orderCode={orderCode}&bookingId={bookingId}"
  }
}